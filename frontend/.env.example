# Environment Configuration for PixelWar

# Sui Network (testnet, devnet, mainnet)
VITE_SUI_NETWORK=testnet

# Smart Contract Addresses (update after deployment)
VITE_PACKAGE_ID=0xYOUR_PACKAGE_ID_HERE
VITE_GAME_ID=0xYOUR_GAME_ID_HERE

# Clock Object (always 0x6 on Sui)
VITE_CLOCK_ID=0x6

# Game Configuration
VITE_CANVAS_SIZE=50
VITE_GAME_DURATION_MS=600000
VITE_MIN_STAKE_SUI=0.1

# Power-up Costs (in SUI)
VITE_POWERUP_SPEED_COST=0.05
VITE_POWERUP_BOMB_COST=0.1
VITE_POWERUP_SHIELD_COST=0.15

# Frontend Configuration
VITE_APP_NAME=PixelWar
VITE_APP_DESCRIPTION=Battle for canvas dominance!

# API Configuration (optional)
# VITE_API_URL=http://localhost:3000
# VITE_WS_URL=ws://localhost:3000

# Analytics (optional)
# VITE_GOOGLE_ANALYTICS_ID=
# VITE_MIXPANEL_TOKEN=
